    <body>

      <h2 id="titel">Loopparcours</h2>


        <% for(var i=0; i < Loopparcours2.features.length; i++) { %>
            <div class='<%- Loopparcours2.features[i].attributes.Naam %> lijst'>
                <header>
                    <h3></h3>
                    <h3><%-Loopparcours2.features[i].attributes.Naam %></h3>
                </header>

                <footer>
                    <small><%- Loopparcours2.features[i].attributes.straat %> <%- Loopparcours2.features[i].attributes.huisnummer %></small>
                </footer>
            </div>
        <% } %>

        <!-- Make sure you put this AFTER Leaflet's CSS -->
        <script src="https://unpkg.com/leaflet@1.3.4/dist/leaflet.js"
        integrity="sha512-nMMmRyTVoLYqjP9hrbed9S+FzjZHW5gY1TWCHA5ckwXZBadntCNs8kEqAWdrb9O7rxbCaA4lKTIWjDXZxflOcA=="
        crossorigin=""></script>
        <script src="js/map.js"></script>

        <script>
            <% for(var i=0; i < Loopparcours2.features.length; i++) { %>
            L.marker([<%- Loopparcours2.features[i].geometry.paths[0][0][1] %>, <%- Loopparcours2.features[i].geometry.paths[0][0][0] %>]).addTo(map)
                .bindPopup("<%- Loopparcours2.features[i].attributes.Naam%>")
            <% } %>
        </script>
    </body>
</html>
